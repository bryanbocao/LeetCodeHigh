public class Solution {
    public List<Integer> getRow(int rowIndex) {
        List<Integer> list = new ArrayList<>(rowIndex+1);
        list.add(1);
        while(rowIndex>=1) {
            int temp = 1;
            for(int i=1;i<list.size();i++) {
                int z = list.get(i);
                list.set(i,list.get(i) + temp);
                temp = z;
            }
            list.add(1);
            rowIndex--;
        }
        return list;
    }
}