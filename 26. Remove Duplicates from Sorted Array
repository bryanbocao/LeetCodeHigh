
/** Two pointers 26. Remove Duplicates from Sorted Array
 *
 *
 */
public class Solution {
    public int removeDuplicates(int[] nums) {
        int p = 0;
        for(int i=1;i<nums.length;i++) {
            if(nums[p]!=nums[i]) {
                nums[++p]=nums[i];
            }
        }
        return p+1;
    }
}